{
  "cSpell.ignoreWords": [
    "antd",
    "reduxjs",
    "scanjs",
    "sonarjs",
    "uuidv",
    "paren"
  ],
  "explorer.fileNesting.patterns": {
    "*.ts": "$(capture).js, $(capture).*.ts, ${capture}.typegen.ts",
    "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
    "*.jsx": "$(capture).js",
    "*.tsx": "$(capture).ts, ${capture}.typegen.ts",
    "package.json": ".*, next-env.d.ts, tsconfig.json, package-lock.json, postcss.config.*, README.MD, tailwind.config.*, next.config.*, jsconfig.*",
  },
  "cSpell.words": [
    "clsx",
    "doke",
    "Doké",
    "lucide",
    "Poké"
  ],
}
